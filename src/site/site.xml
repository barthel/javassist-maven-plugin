<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/DECORATION/1.7.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/DECORATION/1.7.0 http://maven.apache.org/xsd/decoration-1.7.0.xsd"
         name="${project.name}" combine.self="merge">

  <bannerLeft>
    <name>${project.name}</name>
    <alt>${project.name}</alt>
  </bannerLeft>

  <publishDate position="left" format="yyyy-MM-dd" />
  <version position="left" />

  <skin>
    <groupId>org.apache.maven.skins</groupId>
    <artifactId>maven-fluido-skin</artifactId>
  </skin>

  <poweredBy>
    <logo name="Maven" href="https://maven.apache.org" img="/images/logos/maven-feather.png" position="left" alt="Build by Maven" target="_blank" />
    <logo name="icon Systemhaus GmbH" href="http://www.icongmbh.de" img="https://avatars.githubusercontent.com/u/6532939?s=90" alt="icon Systemhaus GmbH" target="_blank" />
  </poweredBy>

  <custom>
    <fluidoSkin>
      <skipGenerationDate>true</skipGenerationDate>
      <breadcrumbDivider>|</breadcrumbDivider>
      <topBarEnabled>false</topBarEnabled>
      <sideBarEnabled>true</sideBarEnabled>
      <sourceLineNumbersEnabled>true</sourceLineNumbersEnabled>
      <leftColumnClass>span3</leftColumnClass>
      <bodyColumnClass>span9</bodyColumnClass>
      <gitHub>
        <projectId>icon-Systemhaus-GmbH/javassist-maven-plugin</projectId>
        <ribbonOrientation>right</ribbonOrientation>
        <ribbonColor>green</ribbonColor>
      </gitHub>
    </fluidoSkin>
  </custom>

  <body>
    <head />

    <breadcrumbs />

    <menu ref="topSideBar" inherit="top" >
      <item name="Home" ref="index" />
    </menu>
    <menu ref="modules" inheritAsRef="false" inherit="bottom" />
    <menu ref="reports" inherit="bottom" />
    <menu ref="parent" />
    <footer />
  </body>
</project>

